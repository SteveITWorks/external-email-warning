<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:type="MailApp">
  <Id>11111111-2222-3333-4444-555555555555</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>YourOrg</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="External Email Warning" />
  <Description DefaultValue="Warns before sending to external recipients."/>
  <IconUrl DefaultValue="https://SteveITWorks.github.io/external-email-warning/icon32.png"/>
  <Hosts><Host Name="Mailbox"/></Hosts>
  <Requirements><Sets DefaultMinVersion="1.8"><Set Name="Mailbox"/></Sets></Requirements>
  <FormSettings><Form xsi:type="ItemSend"><FunctionFile resid="commandsJs"/></Form></FormSettings>
  <Resources>
    <Resource id="commandsJs" type="url" DefaultValue="https://SteveITWorks.github.io/external-email-warning/commands.js"/>
  </Resources>
  <Permissions>ReadWriteMailbox</Permissions>
</OfficeApp>
